<div id="<%= dom_id episode %>">
  <p>
    <strong>Content:</strong>
    <%= episode.content_id %>
  </p>

  <p>
    <strong>Season:</strong>
    <%= episode.season %>
  </p>

  <p>
    <strong>Url:</strong>
    <%= episode.url %>
  </p>
  <!-- <video width="320" height="240" controls>
    <source src="<%= Rails.application.routes.url_helpers.rails_blob_path(episode.file, only_path: true) %>" type="video/mp4">
    Your browser does not support the video tag.
  </video> -->
  <div style="width:320;height:240">
  <%= video_tag rails_blob_url(episode.file), controls: true , style: "width: 320px; height: 240px;"%>
  </div>

  <p>
    <strong>Thumbnail:</strong>
    <%= episode.thumbnail %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= episode.title %>
  </p>

  <p>
    <strong>Runtime:</strong>
    <%= episode.runtime %>
  </p>

  <p>
    <strong>Cast:</strong>
    <%= episode.cast %>
  </p>

  <p>
    <strong>Director:</strong>
    <%= episode.director %>
  </p>

  <p>
    <strong>Story:</strong>
    <%= episode.story %>
  </p>

  <p>
    <strong>Vip status:</strong>
    <%= episode.vip_status %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= episode.status %>
  </p>

</div>
